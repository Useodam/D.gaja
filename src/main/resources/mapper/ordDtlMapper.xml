<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.example.dao.OrdDtlMapper">
    <select id="selectAll" parameterType="String" resultType="OrdDtlDto">
        SELECT *
        FROM ord_dtl
        WHERE ord_cd = #{ordCd}
    </select>
</mapper>